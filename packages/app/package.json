{
  "name": "@maracuja/app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "yarn start:local",
    "configure-app": "yarn configure challenge all",
    "configure": "node scripts/configureApp.js $appId $plateform $withResources",
    "start-orga:prod": "yarn configure crosna && REACT_APP_DEBUG=true REACT_APP_FORCE_ORGA=cros-na HOST=0.0.0.0 PORT=2000 react-app-rewired start",
    "start-orga:dev": "yarn configure testchallenge && REACT_APP_DEBUG=true REACT_APP_FORCE_ORGA=cros-na HOST=0.0.0.0 PORT=2500 react-app-rewired start",
    "start-orga": "yarn start-orga:dev",
    "start-local-orga:dev": "REACT_APP_DEBUG=true REACT_APP_FUNCTIONS_EMULATOR=true REACT_APP_FORCE_ORGA=cros-na HOST=0.0.0.0 PORT=2500 react-app-rewired start",
    "start-local-orga:prod": "REACT_APP_DEBUG=true REACT_APP_FUNCTIONS_EMULATOR=true REACT_APP_FORCE_ORGA=cros-na HOST=0.0.0.0 PORT=2000 react-app-rewired start",
    "start:local": "yarn configure testchallenge && yarn cap-config-local:dev && REACT_APP_FIRESTORE_EMULATOR=true REACT_APP_DEBUG=true PORT=1500 HOST=0.0.0.0 react-app-rewired start",
    "start:dev": "yarn configure testchallenge && yarn cap-config-local:dev && REACT_APP_DEBUG=true PORT=2500 HOST=0.0.0.0 react-app-rewired start",
    "start:prod": "yarn configure challenge && REACT_APP_DEBUG=true PORT=2000 HOST=0.0.0.0 react-app-rewired start",
    "start-local:dev": "yarn configure testchallenge && REACT_APP_DEBUG=true PORT=2500 REACT_APP_FUNCTIONS_EMULATOR=true HOST=0.0.0.0 react-app-rewired start",
    "start-local:prod": "yarn configure challenge && REACT_APP_DEBUG=true PORT=2000 REACT_APP_FUNCTIONS_EMULATOR=true HOST=0.0.0.0 react-app-rewired start",
    "start-local": "yarn start-local:dev",
    "cap-config-local:local": "node scripts/updateCapacitorToLocalhost.js 1500",
    "cap-config-local:dev": "node scripts/updateCapacitorToLocalhost.js 2500",
    "cap-config-local:prod": "node scripts/updateCapacitorToLocalhost.js 2000",
    "live-android": "yarn cap-config-local:dev && yarn build-android",
    "live-android:prod": "yarn cap-config-local:prod && yarn build-android",
    "live-ios": "yarn cap-config-local:local && yarn build-ios",
    "live-ios:dev": "yarn cap-config-local:dev && yarn build-ios",
    "live-ios:prod": "yarn cap-config-local:prod && yarn build-ios",
    "live-mobile:prod": "yarn cap-config-local:prod && yarn build && npx cap sync && npx cap open ios && npx cap open android",
    "live-mobile": "yarn cap-config-local:dev && yarn build && npx cap sync && npx cap open ios && npx cap open android",
    "open-mobile": "npx cap open ios && npx cap open android",
    "create-build": "yarn configure challenge all",
    "build": "CI=false && react-app-rewired build",
    "build-ios": "yarn build && npx cap sync ios && npx cap open ios",
    "build-android": "yarn build && npx cap sync android && npx cap open android",
    "build-mobile": "yarn deploy-config:production && yarn build && npx cap sync",
    "deploy-web": "yarn deploy-web:production",
    "deploy-web:production": "firebase use production && GENERATE_SOURCEMAP=false react-app-rewired build && firebase deploy --only hosting:app",
    "deploy-web:testing": "firebase use development && yarn build && firebase deploy --only hosting:test-app",
    "resources": "node scripts/resources.js cros-na",
    "deploy-config:testing": "ionic deploy configure --app-id=\"f4207600\" --channel-name=\"Testing\"  --update-method=\"none\" --max-store=2",
    "deploy-config:production": "ionic deploy configure --app-id=\"f4207600\" --channel-name=\"Production\"  --update-method=\"none\" --max-store=2",
    "deploy-config:staging": "ionic deploy configure --app-id=\"f4207600\" --channel-name=\"Staging\"  --update-method=\"none\" --max-store=2",
    "set-hosting": "firebase target:apply hosting app maracuja-english-challenge",
    "serve": "http-server build",
    "lint": "standard src --fix",
    "start-test": "yarn configure testchallenge && yarn cap-config-local:dev && REACT_APP_TEST=true REACT_APP_FIRESTORE_EMULATOR=true REACT_APP_DEBUG=true PORT=1500 HOST=0.0.0.0 react-app-rewired start",
    "test": "cross-env FIREBASE_AUTH_EMULATOR_HOST=9099 FIRESTORE_EMULATOR_HOST='localhost:8081' CYPRESS_TEST_UID=jpYEG7kFCzZQ6XsooOQKydIdYW03 cypress open",
    "open-ios": "npx cap open ios",
    "open-android": "npx cap open android"
  },
